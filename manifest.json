{
  "manifest_version": 3,
  "name": "Shaparak Extension",
  "version": "1.0",
  "description": "This extension will not steal your card information :)",
  "permissions": ["scripting"],
  "host_permissions": ["https://sep.shaparak.ir/payment.aspx"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
